# DÃ©finition des workspaces

set $ws1 "1"
set $ws2 "2"
set $ws3 "3"
set $ws4 "4"
set $ws5 "5"
set $ws6 "6"
set $ws7 "7"
set $ws8 "8"
set $ws9 "9"
set $ws10 "10"

# Assignation des workspaces

assign [class="Brave"] $ws1
assign [class="code-oss"] $ws3
for_window [class="Vmware"] move to workspace $ws4
for_window [class = "^discord$" instance="^discord$"] move to workspace $ws9
